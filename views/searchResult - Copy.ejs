    <!--packages start-->
    <section id="pack" class="packages">
        <div class="container-fluid">
            <div class="gallary-header text-center">
                <h2>
                    special packages
                </h2>
                <p>
                    Duis aute irure dolor in velit esse cillum dolore eu fugiat nulla.
                </p>
            </div>
            <!--/.gallery-header-->
            <div class="packages-content">
                <div class="row">
                    <div class="col-md-10">
                        <% scrapped.data.data.forEach(item=>{%>
                            <div class="col-md-4 col-sm-6">
                                <div class="single-package-item" style="min-height:503px;">
                                    <div id="myCarousel_<%=item.id%>" class="carousel slide" data-ride="carousel"
                                        style="min-heigt:145px">
                                        <!-- Indicators -->
                                        <ol class="carousel-indicators">
                                            <%item.cardPhotos.forEach(photo=>{%>
                                                <li data-target="#myCarousel_<%=item.id%>" data-slide-to="0"
                                                    class="active"></li>
                                                <% i++;})%>
                                        </ol>

                                        <!-- Wrapper for slides -->
                                        <div class="carousel-inner">

                                            <% item.cardPhotos.forEach(photo=>{%>
                                                <div class="item">
                                                    <img src="<%=(photo.sizes.urlTemplate.replace('{width}','300')).replace('{height}','100')%>"
                                                        style="width:100%;">
                                                </div>
                                                <%})%>
                                                    <% if(item.cardPhotos.length==0){%>
                                                        <div class="item">
                                                            <img src="" style="width: 100%;height: 141.5px;">
                                                        </div>
                                                        <%}%>
                                        </div>

                                        <!-- Left and right controls -->
                                        <a class="left carousel-control" href="#myCarousel_<%=item.id%>"
                                            data-slide="prev">
                                            <span class="glyphicon glyphicon-chevron-left"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="right carousel-control" href="#myCarousel_<%=item.id%>"
                                            data-slide="next">
                                            <span class="glyphicon glyphicon-chevron-right"></span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                    <div class="single-package-item-txt">
                                        <h3>
                                            <%=item.title%> <span class="pull-right text-success">
                                                    <%=item.priceForDisplay%>
                                                </span><span class="text-danger pull-right"
                                                    style="text-decoration-line: line-through;">
                                                    <%=item.strikethroughPrice%>
                                                </span></span>
                                        </h3>
                                        <div class="bg-info text-center">
                                            <%=item.priceDetails%>
                                        </div>
                                        <div class="packages-para">
                                            <p style="font-size: 14; color: black;">Provided by
                                                <soan style="color: rgb(22, 66, 163); font-size:16px">
                                                    <%=item.provider%>
                                                </soan>
                                            </p>
                                            <p>
                                                <% if(item.primaryInfo!=null){%>
                                                    <i class="fa fa-angle-right"></i>
                                                    <%=item.primaryInfo%>
                                                        <%}%>
                                            </p>
                                            <p>
                                                <% if(item.secondaryInfo!=null){%>
                                                    <i class="fa fa-angle-right"></i>
                                                    <%=item.secondaryInfo%>
                                                        <%}%>
                                            </p>
                                            <p>
                                                <% if(item.accentedLabel!=false){%>
                                                    <i class="fa fa-angle-right"></i>
                                                    <%=item.accentedLabel%>
                                                        <%}%>
                                            </p>
                                        </div>
                                        <!--/.packages-para-->
                                        <div class="packages-review">
                                            <p>
                                                <% for(var i=1; i<=item.bubbleRating.rating;i++){%>
                                                    <i class="fa fa-star"></i>
                                                    <% if(item.bubbleRating.rating-i==0.5){%>
                                                        <i class="fa fa-star-off"></i>
                                                        <%}}%>
                                                            <span class="pull-right text-info">
                                                                <%=item.bubbleRating.count%> reviews
                                                            </span>
                                            </p>
                                        </div>
                                        <!--/.packages-review-->
                                        <div class="about-btn">
                                            <button class="about-view packages-btn">
                                                book now
                                            </button>
                                        </div>
                                        <!--/.about-btn-->
                                    </div>
                                    <!--/.single-package-item-txt-->
                                </div>
                                <!--/.single-package-item-->
                            </div>
                            <%})%>
                    </div>
                    <div class="col-md-10">
                        <button class="btn btn-primary form-btn btn-lg pull-left " type="submit" form="prevpage">Prev</button>
                        <button class="btn btn-primary form-btn btn-lg pull-right " type="submit" form="nextpage">Next</button>
                    </div>
                    <form method="get" action="/api/hotel/searchHotels" id="prevpage" style="display:none">
                        <% Object.keys(params).forEach(index=>{ 
                            if(index=="pageNumber"){%>
                                <input type="hidden" name="<%= index%>" value="<%=Number(params[index].replace(" ",""))-1%>" />
                            <% }else{%>
                            <input type="hidden" name="<%= index%>" value="<%=params[index].replace(" ","")%>" />
                        <%} }) %>
                    </form>

                    <form method="get" action="/api/hotel/searchHotels" id="nextpage" style="display: none;">
                        <% Object.keys(params).forEach(index=>{ 
                            if(index=="pageNumber"){%>
                                <input type="hidden" name="<%= index%>" value="<%=Number(params[index].replace(" ",""))+1%>" />
                            <% }else{%>
                            <input type="hidden" name="<%= index%>" value="<%=params[index].replace(" ","")%>" />
                        <%} }) %>
                    </form>

                </div>
                <!--/.row-->
            </div>
            <!--/.packages-content-->
        </div>
        <!--/.container-->

    </section>
